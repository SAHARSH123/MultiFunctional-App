<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.saharsh.project.Calculator"
    android:background="#ffc107"
    android:padding="0dp">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/editText6"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ffffff"
        android:layout_margin="0dp"
        android:gravity="bottom|right"
        android:textSize="40dp" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="7"
        android:id="@+id/button19"
        android:layout_below="@+id/editText6"
        android:layout_alignLeft="@+id/editText6"
        android:layout_alignStart="@+id/editText6"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="8"
        android:id="@+id/button20"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignBottom="@+id/button19"
        android:layout_toRightOf="@+id/button19"
        android:layout_toEndOf="@+id/button19" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="9"
        android:id="@+id/button21"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignTop="@+id/button20"
        android:layout_toRightOf="@+id/button20"
        android:layout_toEndOf="@+id/button20" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="X"
        android:id="@+id/button22"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignTop="@+id/button21"
        android:layout_toRightOf="@+id/button21"
        android:layout_toEndOf="@+id/button21" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="4"
        android:id="@+id/button23"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button19"
        android:layout_toLeftOf="@+id/button20"
        android:layout_toStartOf="@+id/button20" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="5"
        android:id="@+id/button24"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignBottom="@+id/button23"
        android:layout_toRightOf="@+id/button23"
        android:layout_toEndOf="@+id/button23" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="6"
        android:id="@+id/button25"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignTop="@+id/button24"
        android:layout_toLeftOf="@+id/button22"
        android:layout_toStartOf="@+id/button22" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="-"
        android:id="@+id/button26"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignBottom="@+id/button25"
        android:layout_alignLeft="@+id/button22"
        android:layout_alignStart="@+id/button22" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="1"
        android:id="@+id/button27"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button23"
        android:layout_alignLeft="@+id/button23"
        android:layout_alignStart="@+id/button23" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="2"
        android:id="@+id/button28"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button23"
        android:layout_toRightOf="@+id/button27"
        android:layout_toEndOf="@+id/button27" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="+"
        android:id="@+id/button29"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button24"
        android:layout_alignRight="@+id/button26"
        android:layout_alignEnd="@+id/button26" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="3"
        android:id="@+id/button30"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button24"
        android:layout_toRightOf="@+id/button28"
        android:layout_toEndOf="@+id/button28" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="0"
        android:id="@+id/button31"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="AC"
        android:id="@+id/button32"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button28"
        android:layout_toRightOf="@+id/button31"
        android:layout_toEndOf="@+id/button31" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="="
        android:id="@+id/button33"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button29"
        android:layout_toLeftOf="@+id/button29"
        android:layout_toStartOf="@+id/button29" />

    <Button
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:text="/"
        android:id="@+id/button34"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:background="#5b5757"
        android:layout_below="@+id/button29"
        android:layout_toRightOf="@+id/button33"
        android:layout_toEndOf="@+id/button33" />

    <Button

        android:layout_width="94dp"
        android:layout_height="60dp"
        android:background="#5b5757"
        android:text="BACK"
        android:id="@+id/button35"
        android:textColor="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/editText6"
        android:layout_alignStart="@+id/editText6"
        android:textSize="20dp" />

</RelativeLayout>
